  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonidasKyrkos/VideoTrans.git"
  },
  "keywords": [
    "video+transitions",
    "video+slider",
    "video+captions"
  ],
  "author": "Leonidas Kyrkos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeonidasKyrkos/VideoTrans/issues"
  },
  "homepage": "https://github.com/LeonidasKyrkos/VideoTrans#readme",
  "dependencies": {
    "jquery": "1.12.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",   
    "babelify": "^7.3.0",
    "browser-sync": "^2.8.2",
    "browserify": "^12.0.1",
    "debowerify": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.0.4",
    "gulp-streamify": "^1.0.0",
    "gulp-uglify": "^1.4.0",
    "gulp-util": "^3.0.6",
    "node-bourbon": "^4.2.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "browserify": {
    "transform": [
      "debowerify"
    ]
